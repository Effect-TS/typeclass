{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noErrorTruncation": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2021",
    "lib": ["ES2021"],
    "paths": {
      "@effect/typeclass": ["../src/index.ts"],
      "@effect/typeclass/test/*": ["../test/*"],
      "@effect/typeclass/examples/*": ["../examples/*"],
      "@effect/typeclass/*": ["../src/*"]
    }
  }
}
